{
  "name": "@stlmpp/di",
  "version": "0.3.0",
  "keywords": [
    "DI",
    "Dependency injection"
  ],
  "license": "MIT",
  "author": "stLmpp",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "start": "node --enable-source-maps dist/index.js",
    "test": "jest",
    "test:ci": "jest --silent",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "watch": "tsup --watch"
  },
  "dependencies": {
    "@abraham/reflection": "^0.12.0",
    "tslib": "^2.5.0",
    "type-fest": "^3.9.0"
  },
  "devDependencies": {
    "@jest/types": "^29.5.0",
    "@swc/core": "~1.3.56",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  }
}
